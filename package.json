{
  "name": "anki-interview-app",
  "version": "0.1.0",
  "private": true,
  "description": "Spaced repetition app for interview questions - Full Stack Application",
  "scripts": {
    "dev": "pnpm --filter anki-interview-app dev",
    "dev:backend": "pnpm --filter anki-interview-backend dev",
    "dev:frontend": "pnpm --filter anki-interview-app dev",
    "preview:frontend": "pnpm --filter anki-interview-app preview",
    "build": "pnpm -r build",
    "build:frontend": "pnpm --filter anki-interview-app build",
    "build:backend": "pnpm --filter anki-interview-backend deploy",
    "install:all": "pnpm install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^15.15.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.47.0",
    "wrangler": "^4.50.0"
  }
}
